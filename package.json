{
  "name": "sudorandom.dev",
  "version": "1.0.0",
  "description": "",
  "cuttlebelle": {
    "site": {
      "markdownRenderer": "markdown.js",
      "globalProp": {
        "sitename": "sudorandom"
      }
    }
  },
  "scripts": {
    "docs": "cuttlebelle docs",
